# Copy this file to `.env` and fill in values. Do NOT commit your real `.env` with secrets.

# Which environment to use: "demo" or "prod"
KALSHI_ENV=demo

# Demo credentials
DEMO_KEYID=your-demo-key-id-here
DEMO_KEYFILE=path/to/your/demo_private_key.pem

# Production credentials
PROD_KEYID=your-prod-key-id-here
PROD_KEYFILE=path/to/your/prod_private_key.pem
